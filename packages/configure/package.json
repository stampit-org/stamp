{
  "name": "@stamp/configure",
  "version": "1.0.2",
  "license": "MIT",
  "description": "Access configuration of your stamps anywhere",
  "main": "index.js",
  "files": [
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stampit-org/stamp/tree/master/packages/configure"
  },
  "keywords": [
    "compose",
    "configure"
  ],
  "scripts": {
    "build": "tsc",
    "link": "npm run build && lerna link"
  },
  "dependencies": {
    "@stamp/compose": "^1.0.2",
    "@stamp/privatize": "^1.0.3"
  },
  "devDependencies": {
    "xo": "^0.38.2"
  },
  "xo": {
    "ignores": [
      "**.js",
      "**.d.ts",
      "__tests__"
    ],
    "space": 2,
    "rules": {
      "capitalized-comments": "warn",
      "import/extensions": "warn"
    },
    "prettier": true
  }
}
